{
  "name": "@decorum/dynamodb-seeder",
  "version": "0.1.0",
  "description": "Command-line interface for seeding data into DynamoDB",
  "main": "dist/cli.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
